<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>..\..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <TargetFramework>net461</TargetFramework>
    <OutputPath>..\..\..\Xpand.DLL\</OutputPath>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>pdbonly</DebugType>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
    <PackageReference Include="Xpand.VersionConverter" Version="1.0.36" />
    <PackageReference Include="DevExpress.ExpressApp.ConditionalAppearance" Version="19.1.7" />
    <PackageReference Include="DevExpress.Data" Version="19.1.7" />
    <PackageReference Include="DevExpress.ExpressApp.Security" Version="19.1.7" />
    <PackageReference Include="DevExpress.ExpressApp.Validation" Version="19.1.7" />
    <PackageReference Include="DevExpress.ExpressApp.Security.Xpo" Version="19.1.7" />
    <PackageReference Include="DevExpress.ExpressApp.Xpo" Version="19.1.7" />
    <PackageReference Include="DevExpress.ExpressApp" Version="19.1.7" />
    <PackageReference Include="DevExpress.Persistent.Base" Version="19.1.7" />
    <PackageReference Include="DevExpress.Xpo" Version="19.1.7" />
    <PackageReference Include="Xpand.XAF.Modules.ModelViewInheritance" Version="1.1.54.1" />
    <PackageReference Include="Fasterflect.Xpand" Version="2.0.6" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Configuration" />
    <Reference Include="Xpand.Persistent.Base">
      <HintPath>..\..\..\Xpand.Dll\Xpand.Persistent.Base.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Xpand.Utils">
      <HintPath>..\..\..\Xpand.Dll\Xpand.Utils.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Xpand.Xpo">
      <HintPath>..\..\..\Xpand.Dll\Xpand.Xpo.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="AuthenticationProviders\AutoAuthenticationStandard.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="AuthenticationProviders\XpandAuthenticationStandard.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\AnonymousLoginPermissionController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\MyDetailsController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\ShowNavigationItemController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\MyDetailsPermissionController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\DefaultRolePermissionsController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\DefaultRolePermissionsController.Designer.cs">
      <DependentUpon>DefaultRolePermissionsController.cs</DependentUpon>
    </Compile>
    <Compile Update="Controllers\RememberMeController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Module.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="XpandSecurityModuleBase.cs">
      <SubType>Component</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Model.DesignedDiffs.xafml">
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <None Include="Properties\Xpand.VersionConverter.ReadMe.md" />
  </ItemGroup>
</Project>